{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "socket": "tsx src/server/socketServer.ts",
    "dev": "concurrently \"npm:dev:next\" \"npm:socket\"",
    "server": "node server.js",
    "test:e2e": "playwright test",
    "build:server": "tsc --project tsconfig.build.json",
    "start:server": "node dist/server/socketServer.js",
    "prisma:generate": "prisma generate",
    "build": "prisma generate && next build && npm run build:server",
    "start": "concurrently \"npm run start:server\" \"next start\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@prisma/client": "^6.8.2",
    "clsx": "^2.1.1",
    "create-server": "^1.0.2",
    "express": "^5.1.0",
    "formidable": "^3.5.4",
    "lucide-react": "^0.523.0",
    "next": "^15.3.2",
    "prisma": "^6.8.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.5.2",
    "react-player": "^2.16.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.x",
    "@types/react": "^18.x",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18",
    "@types/socket.io": "^3.0.1",
    "@types/streamifier": "^0.1.2",
    "autoprefixer": "^10",
    "concurrently": "^9.1.2",
    "eslint": "^8",
    "eslint-config-next": "13.5.11",
    "postcss": "^8",
    "tailwindcss": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.3.0"
  }
}
